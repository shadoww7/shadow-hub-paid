/* FunÃ§Ãµes principais */
function setupMain() {
    loadScript('https://raw.githubusercontent.com/DarkMod3/KhanFucker/refs/heads/Main/Plugins/questionSpoof.js', 'questionSpoof');
    loadScript('https://raw.githubusercontent.com/DarkMod3/KhanFucker/refs/heads/Main/Plugins/videoSpoof.js', 'videoSpoof');
    loadScript('https://raw.githubusercontent.com/DarkMod3/KhanFucker/refs/heads/Main/Plugins/spoofUser.js', 'spoofUser');
    loadScript('https://raw.githubusercontent.com/DarkMod3/KhanFucker/refs/heads/Main/Plugins/autoAnswer.js', 'autoAnswer');
}

/* FunÃ§Ã£o para carregar scripts dinamicamente */
function loadScript(url, label) {
    return fetch(url)
        .then(response => response.text())
        .then(script => {
            console.log(`Carregando o script: ${label}`);
            eval(script); // Executa o cÃ³digo carregado
        })
        .catch(error => {
            console.error(`Erro ao carregar o script ${label}:`, error);
        });
}

/* FunÃ§Ã£o para mostrar uma mensagem de carregamento */
function showLoadingMessage() {
    console.log("ğŸ”„ Carregando KhanFucker...");
    // Aqui vocÃª pode incluir uma tela de carregamento se necessÃ¡rio
}

/* FunÃ§Ã£o para inicializar o sistema */
function initializeSystem() {
    showLoadingMessage();
    setupMain();
    console.log("KhanFucker iniciado!");
}

/* Chama a funÃ§Ã£o principal */
initializeSystem();
